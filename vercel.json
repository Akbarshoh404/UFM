{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel-python",
      "config": {
        "maxLambdaSize": "15mb4",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
}